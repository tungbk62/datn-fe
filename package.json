    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.0.0",
    "dayjs": "^1.11.1",
    "dotenv": "^16.0.0",
    "formik": "^2.2.9",
    "fs-extra": "^10.1.0",
    "html-react-parser": "^3.0.9",
