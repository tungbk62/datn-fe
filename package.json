{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "rm -r -f .next && next build",
    "start": "next start -p 3063",
    "build-and-start": "rm -r -f .next && next build && next start -p 3063",
    "type-check": "tsc",
    "typeorm": "typeorm-ts-node-commonjs",
    "dev:server": "cross-env NODE_ENV=dev nodemon --exec ts-node -r tsconfig-paths/register ./src/server/server.ts",
    "build:server": "cross-env NODE_ENV=production rm -r -f dist && tsc --project tsconfig.server.json && cp src/server/swagger.json dist/",
    "start:custom": "cross-env NODE_ENV=production node -r ./tsconfig-paths.js dist/server.js",
    "build:custom": "rm -r -f .next && next build && cross-env NODE_ENV=production rm -r -f dist && tsc --project tsconfig.server.json && cp src/server/swagger.json dist/",
    "migrate": "rm -rf build && yarn build && yarn typeorm migration:generate ./src/server/migrations/added-user-entity -d ./src/server/utils/data-source.ts",
    "db:push": "rm -rf build && yarn build && yarn typeorm migration:run -d ./src/server/utils/data-source.ts"
  },
  "dependencies": {
